{
  "name": "@supernode/shared",
  "version": "1.0.0",
  "description": "共享类型和常量",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "reactflow": "^11.11.4",
    "gray-matter": "^4.0.3",
    "mdast": "^2.3.2",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5"
  }
}